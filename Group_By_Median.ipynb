   "source": [
    "import pandas as pd\n",
    "\n",
    "data = {\n",
    "    'Category': ['A', 'A', 'B', 'B', 'A', 'B', 'A', 'B'],\n",
    "    'Value': [1, 2, 3, 4, None, 6, 7, None]\n",
    "}\n",
    "df = pd.DataFrame(data)\n",
    "\n",
    "print(df)\n",
    "\n",
    "\n",
    "df['Value'] = df.groupby('Category')['Value'].transform(lambda x: x.fillna(x.median()))\n",
    "\n",
    "print(df)\n"
