    "from sklearn.preprocessing import StandardScaler\n",
    "\n",
    "data = [[10], [20], [30], [40], [50]]\n",
    "\n",
    "scaler = StandardScaler()\n",
    "\n",
    "scaled_data = scaler.fit_transform(data)\n",
    "\n",
    "print(\"Original Data:\")\n",
    "print(data)\n",
    "\n",
    "print(\"\\nScaled Data (Standard Scaling):\")\n",
    "print(scaled_data)"
   ]
