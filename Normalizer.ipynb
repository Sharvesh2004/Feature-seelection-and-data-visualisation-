   "source": [
    "from sklearn.preprocessing import Normalizer\n",
    "import numpy as np\n",
    "\n",
    "\n",
    "data = np.array([[1, 2], [3, 4], [5, 6]])\n",
    "\n",
    "scaler = Normalizer()\n",
    "\n",
    "scaled_data = scaler.fit_transform(data)\n",
    "\n",
    "print(\"Original Data:\")\n",
    "print(data)\n",
    "\n",
    "print(\"\\nScaled Data (Normalizer Scaling):\")\n",
    "print(scaled_data)"
   ]
